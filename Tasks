//Time of seasons
function month(m) {
    if (m == 12 || m <= 2) {
        return "Winter";
    }else if (m == 3 || m <= 5 ) {
        return "Spring";
    }else if (m == 6 || m <= 8 ) {
        return "Summer";
    }else if (m == 9 || m <= 11 ) {
        return "Autumn";
    }   return "NewMonth";
}
console.log(month(21));
